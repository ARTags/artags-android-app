<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          android:versionCode="1"
          android:versionName="1.0"

          package="com.artgameweekend.projects.art">
    <application>
        <activity android:name=".MainActivity" android:icon="@drawable/icon"  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".FingerPaint" android:label="@string/activity_paint">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.PAINT" />
            </intent-filter>
        </activity>

        <activity android:name=".ShowPicture" android:label="@string/activity_ShowImage">
            <intent-filter>
                <action android:name="wikitudeapi.SHOWIMAGE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
    </application>
</manifest>
